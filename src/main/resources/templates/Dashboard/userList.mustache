{{> header}}
  <h1>Usuarios registrados</h1>
  <table class="table table-dark">
    <thead>
      <tr class="table-primary">
          <th scope="col">id</th>
          <th scope="col">Nombre usuario</th>
          <th scope="col">Email</th>
          <th scope="col">Tipo usuario</th>
        </tr>
      </thead>
    <tbody>
      {{#usuarios}}
        <tbody>
          <tr>
            <td>{{id}}</td>
            <td>{{name}}</td>
            <td>{{email}}</td>
            <td>{{type}}</td>
          </tr>
        </tbody>
      {{/usuarios}}
    </tbody>
    </table>
{{> footer}}
